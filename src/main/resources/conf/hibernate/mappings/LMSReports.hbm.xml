<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="edu.ucdavis.ss.lmsreports.domain">


	

	<class name= "ActivityType" table="actlabel">
	   <id name="activityTypeId" column="actlabel_pk"  />
	    <property name="activityTypeDesc" column="actlabel_name" type="java.lang.String"/>
	</class>
	
	<class name="Domain" table ="org">
	  <id name="domainId" column="org_pk" />
	  <property name="domainName" column="org_name" />
	 
	</class>
</hibernate-mapping>
